---
#
# Ansible managed
#
exclude_paths:
  - meta/preferences.yml
  - molecule/default/prepare.yml
  - molecule/default/converge.yml
  - molecule/default/verify.yml
  - molecule/default/collections.yml
  - .tox
  - .cache
  - .github
  - requirements.yml

skip_list:
  - yaml[truthy]
  - no-changed-when
  - galaxy[version-missing]
  - schema[galaxy]
  - galaxy[tags]
  - galaxy[no-changelog]
  - schema[requirements]

enable_list:
  - name[prefix]
